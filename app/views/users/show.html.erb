<h1 class="mt-5 pt-5">Tweet</h1>

<p style="color: green"><%= notice %></p>

<%= render @user %>

<div>
  <%= link_to "Editar Tweet", edit_user_path(@user) %> |
  <%= link_to "Volver", users_path %>
</div>

<div class="mt-3">
  <%= button_to "Eliminar Tweet", @user, method: :delete, class: 'btn btn-danger' %>
</div>
